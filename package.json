    "base-64": "0.1.0",
    "buffer": "5.6.0",
    "core-js": "^3",
    "epubjs": "0.3.93",
    "event-emitter": "^0.3.5",
    "events": "3.1.0",
    "i": "0.3.6",
    "jszip": "3.10.1",
    "lodash": "^4.17.20",
    "path-webpack": "0.0.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-blob-util": "0.19.11",
    "react-native-orientation-locker": "1.7.0",
