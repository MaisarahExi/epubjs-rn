  "main": "./components/index.js",
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start",
    "build": "./node_modules/.bin/babel src/ -d components/ --delete-dir-on-start --compact false && cp -R ./src ./components/src/",
